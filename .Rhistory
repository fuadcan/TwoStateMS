# Reshaping tables into similar forms
correctRes <- function(res){
# Reformats the table of estimates for it to be in "dH,dL,PHH,PLL, ..." order
if(ncol(res) == 7){
res[res[,1]>res[,2],] <- matrix(res[res[,1]>res[,2],],,ncol(res))[,c(2,1,4,3,5:ncol(res))]
} else {
res[res[,1]>res[,2],] <- matrix(res[res[,1]>res[,2],],,ncol(res))[,c(2,1,4,3,5,7,6,8)]}
return(res)
}
